           <script type="text/javascript">
                var contentId;
                var globalDestination = destinationCode; //"discovery_web"; //PAGE IS RESPONSIBLE TO PASS TO PLAYER & 42 CLIENT API
                var globalPlatform = "desktop"; //PAGE IS RESPONSIBLE IN PASSING TO PLAYER & 42 CLIENT API 
                GetQuerystring = function (key, default_) {
                    if (default_ == null) default_ = "";
                    key = key.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
                    var regex = new RegExp("[\\?&]" + key + "=([^&#]*)");
                    var qs = regex.exec(window.location.href);
                    if (qs == null)
                        return default_;
                    else
                        return qs[1];
                };
                function getUrlVars() {
                    var vars = [], hash;
                    var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
                    for (var i = 0; i < hashes.length; i++) {
                        hash = hashes[i].split('=');
                        vars.push(hash[0]);
                        vars[hash[0]] = hash[1];
                    }
                    return vars;
                }

                var contentIdQuery = getUrlVars()['vid'];

                if (contentIdQuery == "" | contentIdQuery == null) {
                    contentId = parseInt(GetQuerystring("contentId", '541879')); //"openingClip" being passed in through the back end code via the web part in the editor field
                    //alert(contentId);
                } else {
                    contentId = parseInt(GetQuerystring("contentId", contentIdQuery)); //322164
                }

                if (!/MSIE (\d+\.\d+);/.test(navigator.userAgent)) { //test for MSIE x.x;
                    //adding querystring and paramater to IE browser not possible yet, so catch history.pushstate in this IF statement for all NON IE browsers
                    history.pushState("", "", "/" + window.location.pathname.substr(1) + "?vid=" + contentId + "");
                }
                //window.location.assign(window.location.href += '?vid=' + contentId + '');
                //History.pushState(null, null, "?vid=" + contentId + "");

                initPlayer = function () {
                    player.embedPlayer();
                };
                var player = {};
                player.playerObjList = {};

                player.PlayerReady = function () {
                    player.initParms = { destinationCode: globalDestination, platformCode: globalPlatform, autoPlay: 1, isAuthenticationRequired: false };
                    var item = { id: contentId };
                    player.playerObjList['vplayer'].playMediaItem(item, player.initParms);
                };
                player.onPlayerReady = function () {
                    //_log("player ready");
                    player.PlayerReady();
                };
                player.onVideoEnded = function (infoObj) {
                    //_log("player ended");
                };
                player.onContentComplete = function (infoObj) {
                    //_log("content complete");
                    //alert('contentComplete');
                    var nextVid = jQuery('.vh-video-item .play').parent().parent().parent().find('a').attr('href');
                    if (nextVid.length > 0) {
                        var clipId = nextVid.replace(/[^0-9]/g, '');
                        var url = window.location.href;
                        url = url.substring(0, url.indexOf('?')); //this keeps urls from appending more and more ?vid=123456
                        var parameter = "?vid=" + clipId + "";
                        //alert(url);
                        window.location = url + parameter;
                    }
                };
                player.onVideoStarted = function (infoObj) {
                    //_log("player started");
                };
                player.onVideoError = function (infoObj) {
                    //_error("player error");
                };
                player.ajaxCrossDomainCors = function (serviceURL) {
                    try {
                        player.JqueryCallBack = function () {
                            _log("api loading success ");
                            return;
                        };
                        var ajaxPars = {
                            url: serviceURL,
                            cache: true,
                            contentType: "application/json",
                            jsonpCallback: "player.JqueryCallBack",
                            error: function (XMLHttpRequest, textStatus, errorThrown) {
                                if (textStatus == "parsererror") return;
                                //_error("loading error: " + info + " " + textStatus);
                            }
                        };
                        ajaxPars.type = "GET";
                        ajaxPars.dataType = "jsonp";
                        ajaxPars.jsonp = "callback";
                        $.ajax(ajaxPars);

                    } catch (err) {
                        throw (err);
                    }
                };

                player.embedPlayer = function () {
                    var adrule = GetQuerystring("adrule", "generic");
                    var mainApp;
                    var playlistObj;

                    var mpFlashVars = {};
                    mpFlashVars.enablelog = 1;
                    mpFlashVars.loginterval = 1;
                    mpFlashVars.cms = "fortytwo";

                    mainApp = etsapi.embedPlayer('vplayer', 'mediaplayerdiv', 620, 348, globalDestination, '', { flashVars: mpFlashVars, adRule: adrule });
                    mainApp.registerEvent(MediaPlayerControllerEvent.PLAYER_READY, this, player.onPlayerReady);
                    mainApp.registerEvent(MediaPlayerControllerEvent.VIDEO_STARTED, this, player.onVideoStarted);
                    mainApp.registerEvent(MediaPlayerControllerEvent.VIDEO_ENDED, this, player.onVideoEnded);
                    mainApp.registerEvent(MediaPlayerControllerEvent.ERROR, this, player.onVideoError);
                    mainApp.registerEvent(MediaPlayerControllerEvent.CONTENT_COMPLETE, this, player.onContentComplete);

                    player.playerObjList['vplayer'] = mainApp;
                    //etsapi.setAdZone("vplayer", "suits-videohub");
                    etsapi.setAdCustomParams("vplayer", "envr=prod");
                    etsapi.setAdSite("vplayer", "ctv.discoverychannel.ca");
                    etsapi.setAdZone("vplayer", "discovery/daily-planet/video");

                };

                $(document).ready(function () {
                    $(document).on('onready.etsapi', player.embedPlayer);
                    player.ajaxCrossDomainCors("http://player.9c9media.com/channels/" + destinationCode + "/apiInterface.js");
                    //player.ajaxCrossDomainCors("/Custom/Global/assets/js/etsAxisApi.js");
                });
        </script> 
