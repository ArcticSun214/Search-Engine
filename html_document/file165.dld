            var getWeather = function (zipcode) {                
                var locationUrl = apiUrl + "/locations/v1/US/search?q=" + zipcode + "&apikey=" + apiKey;
                //var locationKey = "";
                $.ajax({
                    type: "GET",
                    url: locationUrl,
                    dataType: "jsonp",
